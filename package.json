{
  "name": "storefront",
  "version": "0.1.8",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier \"./src/**/*.{ts,tsx,js,jsx,json,graphql,md}\" --check",
    "prettier:autofix": "npm run prettier -- --write",
    "bump": "npm version patch",
    "eslint-root": "eslint \"./*.js\" --cache --cache-location ./.cache/eslint/root-files",
    "eslint-project": "eslint \"./{libs,pages,src}/**/**.{js,ts,tsx}\" --cache --cache-location ./.cache/eslint/project-files",
    "eslint:autofix": "npm run eslint-root -- --fix && npm run eslint-project -- --fix",
    "jest": "jest",
    "test:autofix": "npm run prettier:autofix && npm run eslint:autofix && npm run jest"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "@types/cheerio": "^0.22.24",
    "@types/enzyme": "^3.10.9",
    "@wezom/eslint-config": "^8.0.0",
    "@wezom/eslint-config-ts": "^8.0.0",
    "@wezom/eslint-config-ts-react": "^8.0.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "cheerio": "^1.0.0-rc.5",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.0",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.0"
  }
}
